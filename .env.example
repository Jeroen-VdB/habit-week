# Google OAuth Client ID from Google Cloud Console
PUBLIC_GOOGLE_CLIENT_ID=your-client-id-here.apps.googleusercontent.com

# Redirect URI (optional, defaults to current origin)
PUBLIC_REDIRECT_URI=http://localhost:4321

# Sentry DSN (optional)
PUBLIC_SENTRY_DSN=

# Umami Analytics (optional)
PUBLIC_UMAMI_WEBSITE_ID=
# If self-hosting Umami, provide your URL (optional, defaults to Umami Cloud)
PUBLIC_UMAMI_URL=